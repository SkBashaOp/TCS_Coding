from collections import *
def solve(arr):
    store=defaultdict(int)
    for num in arr:
        store[num]+=1
    ans= sorted(store.items(), key = lambda item : item[1], reverse=True)  
    print(ans)
    for num in ans:
        first1,second1 = num
        for i in range(second1):
            print(first1, end=" ")
arr=[1,1,2,2,2,3,2,3,1,3,2] 
solve(arr)
        
